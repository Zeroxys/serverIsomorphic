{
  "name": "pwa",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {

    "//actitudfem dev": "Estos son los scripts para actitud fem",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch all/actitudfem/build --exec \"node all/actitudfem/build/bundle.js\"",
    "dev:build-server": "cd all/actitudfem/src && webpack --mode development --config webpack.server.js --watch",
    "dev:build-client": "cd all/actitudfem/src && webpack --mode development --config webpack.client.js --watch",
    "//actitudfem prod": "Estos son los scripts para producction actitudfem",
    "prod": "npm-run-all --parallel prod:*",
    "prod:server": "nodemon --watch all/actitudfem/build --exec \"node all/actitudfem/build/bundle.js\"",
    "prod:build-server": "cd all/actitudfem/src && webpack --mode production --config webpack.server.js --watch",
    "prod:build-client": "cd all/actitudfem/src && webpack --mode production --config webpack.client.js --watch",

    "//actitudfem anakin": "Estos son los scripts para actitud fem",
    "anakin": "npm-run-all --parallel anakin:*",
    "anakin:build-server": "cd all/actitudfem/src && webpack --mode production --config webpack.server.js --progress",
    "anakin:build-client": "cd all/actitudfem/src && webpack --mode production --config webpack.client.js --progress",
    "anakin:server": "node all/actitudfem/build/bundle.js"
  },
  "author": " <miguelzavalac@gmail.com>",
